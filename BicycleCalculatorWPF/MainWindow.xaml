<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:res="clr-namespace:BicycleCalculatorWPF.Properties"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Name="MainWindow1" x:Class="BicycleCalculatorWPF.MainWindow"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:WpfApplication2"
        Title="{x:Static res:Resources.StringTitle}" Height="711" Width="1000" Loaded="Form1_Load" Closed="Window_Closed_1" Icon="Resources/自行车计算器s.ico" FontFamily="Microsoft YaHei UI" Background="#FFF0F0F0" Closing="Window_Closing_1" WindowStartupLocation="CenterScreen">
    <Window.DataContext>
        <local:MainViewModel/>
    </Window.DataContext>
    <Window.Resources>
        <ResourceDictionary/>
    </Window.Resources>
    <Grid x:Name="MainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <Menu VerticalAlignment="Top" Padding="6,2,0,2">
            <MenuItem Header="{x:Static res:Resources.StringFile}" Padding="8,2,8,1">
                <MenuItem Header="{x:Static res:Resources.StringOpen}" Visibility="Collapsed"/>
                <MenuItem x:Name="saveToolStripMenuItem" Header="{x:Static res:Resources.StringExport}" Click="saveToolStripMenuItem_Click"/>
                <MenuItem Header="{x:Static res:Resources.StringEditDataSet}">
                    <MenuItem Header = "{x:Static res:Resources.StringCassette}" Click="EditCassetteMenuItem_Click"/>
                    <MenuItem Header = "{x:Static res:Resources.StringCranksets}" Click="EditCranksetsMenuItem_Click"/>
                    <MenuItem Header = "{x:Static res:Resources.StringInternalHub}" Click="EditInternalHubMenuItem_Click"/>
                    <MenuItem Header = "{x:Static res:Resources.StringTire}" Click="EditTireMenuItem_Click"/>
                    <Separator/>
                    <MenuItem Header = "{x:Static res:Resources.StringHubs}" Click="EditHubsMenuItem_Click"/>
                    <MenuItem Header = "{x:Static res:Resources.StringRims}" Click="EditRimsMenuItem_Click"/>
                </MenuItem>
                <MenuItem Header="{x:Static res:Resources.StringLanguage}" SubmenuOpened="LanguageMenuItem_Click">
                    <MenuItem Header = "{x:Static res:Resources.StringDefault}" Click="DftLanguageMenuItem_Click"/>
                    <Separator/>
                    <MenuItem Header = "English" Click="EngMenuItem_Click"/>
                    <MenuItem Header = "简体中文" Click="ChnMenuItem_Click"/>
                    <MenuItem Header = "繁體中文" Click="ChtMenuItem_Click"/>
                </MenuItem>
                <Separator/>
                <MenuItem x:Name="exitToolStripMenuItem" Header="{x:Static res:Resources.StringExit}" Click="exitToolStripMenuItem_Click"/>
            </MenuItem>
            <MenuItem x:Name="ViewMenuItem" Header="{x:Static res:Resources.StringView}" SubmenuOpened="ViewMenuItem_SubmenuOpened" Padding="8,2,8,1">
                <MenuItem x:Name="internalHubsToolStripMenuItem" Header="{x:Static res:Resources.StringInternalHub}" IsCheckable="True" IsChecked="True" Click="internalHubsToolStripMenuItem_Click" />
                <MenuItem x:Name="chartToolStripMenuItem" Header="{x:Static res:Resources.StringChart}" IsCheckable="True" IsChecked="True" Click="chartToolStripMenuItem_Click" />
            </MenuItem>
            <MenuItem x:Name="RusultMenuItem" Header="{x:Static res:Resources.StringResult}"  Padding="8,2,8,1" >
                <MenuItem x:Name="selectToolStripMenuItem" Header="{x:Static res:Resources.StringColumn}" SubmenuOpened="selectToolStripMenuItem_SubmenuOpened">
                    <MenuItem Header="ALL"/>
                </MenuItem>
                <MenuItem x:Name="unitToolStripMenuItem" Header="{x:Static res:Resources.StringUnit}" SubmenuOpened="unitToolStripMenuItem_SubmenuOpened">
                    <MenuItem x:Name="kMHToolStripMenuItem" Header="KM/H" Click="kMHToolStripMenuItem_Click"/>
                    <MenuItem x:Name="mPHToolStripMenuItem" Header="MPH" Click="mPHToolStripMenuItem_Click"/>
                </MenuItem>
            </MenuItem>
            <MenuItem Header="{x:Static res:Resources.StringChart}"  Padding="8,2,8,1" SubmenuOpened="PlotToolStripMenuItem_SubmenuOpened">
                <MenuItem Header="{x:Static res:Resources.StringDisAll}" Click="DisAllToolStripMenuItem_Click" Tag="2"/>
                <Separator Tag="2"/>
                <MenuItem Header="{x:Static res:Resources.StringLRSide}" SubmenuOpened="LRSideDisToolStripMenuItem_Click" Tag="2">
                    <MenuItem Header="{x:Static res:Resources.StringRight}" IsCheckable="True" Click="DisLeftToolStripMenuItem_Click"/>
                    <MenuItem Header="{x:Static res:Resources.StringLeft}" IsCheckable="True" Click="DisRightToolStripMenuItem_Click"/>
                </MenuItem>
                <MenuItem Header="{x:Static res:Resources.StringDisItem}" SubmenuOpened="DisItemStripMenuItem_Click" Tag="2">
                    <MenuItem Header="{x:Static res:Resources.StringRimline}" IsCheckable="True" Click="RimlineToolStripMenuItem_Click"/>
                    <MenuItem Header="{x:Static res:Resources.StringRimhole}" IsCheckable="True" Click="RimholeToolStripMenuItem_Click"/>
                    <MenuItem Header="{x:Static res:Resources.StringHubline}" IsCheckable="True" Click="HublineToolStripMenuItem_Click"/>
                    <MenuItem Header="{x:Static res:Resources.StringHubhole}" IsCheckable="True" Click="HubholeToolStripMenuItem_Click"/>
                    <MenuItem Header="{x:Static res:Resources.StringSpokesline}" IsCheckable="True" Click="SpolineToolStripMenuItem_Click"/>
                </MenuItem>
                <Separator Tag="2"/>
                <MenuItem Header="{x:Static res:Resources.StringResetAxis}" Click="ResetAxisToolStripMenuItem1_Click" Tag="2"/>

                <MenuItem x:Name="AddCurveToolStripMenuItem" Header="{x:Static res:Resources.StringAddCurve}" Click="AddCurveToolStripMenuItem_Click" Tag="1"/>
                <MenuItem x:Name="ClrCurveToolStripMenuItem" Header="{x:Static res:Resources.StringClrCurve}" Click="ClrCurveToolStripMenuItem_Click" Tag="1"/>
                <Separator Tag="1"/>
                <MenuItem x:Name="BranchDisToolStripMenuItem" Header="{x:Static res:Resources.StringBranchDisplay}" SubmenuOpened="BranchDisToolStripMenuItem_Click" Tag="1">
                    <MenuItem x:Name="WithCrankToolStripMenuItem" Header="{x:Static res:Resources.StringBranchfr}" IsCheckable="True" Click="WithCrankToolStripMenuItem_Click"/>
                    <MenuItem x:Name="WithCassToolStripMenuItem" Header="{x:Static res:Resources.StringBranchbk}" Visibility="Collapsed" IsCheckable="True" Click="WithCassToolStripMenuItem_Click"/>
                    <MenuItem x:Name="WithInterToolStripMenuItem" Header="{x:Static res:Resources.StringBranchin}" IsCheckable="True" Click="WithInterToolStripMenuItem_Click"/>
                    <Separator/>
                    <MenuItem x:Name="NoneToolStripMenuItem" Header="{x:Static res:Resources.StringNone}" IsCheckable="True" IsChecked="True" Click="NoneToolStripMenuItem_Click"/>
                </MenuItem>
                <MenuItem x:Name="YAxisToolStripMenuItem" Header="{x:Static res:Resources.StringYAxis}" SubmenuOpened="YAxisToolStripMenuItem_Click" Tag="1">
                    <MenuItem x:Name="SRToolStripMenuItem" Header="{x:Static res:Resources.StringSpdRatio}" IsCheckable="True" IsChecked="True" Click="SRToolStripMenuItem_Click"/>
                    <MenuItem x:Name="GIToolStripMenuItem" Header="{x:Static res:Resources.StringGI}" IsCheckable="True" Click="GIToolStripMenuItem_Click"/>
                    <MenuItem x:Name="GRToolStripMenuItem" Header="{x:Static res:Resources.StringGearRatio}" IsCheckable="True" Click="GRToolStripMenuItem_Click"/>
                    <MenuItem x:Name="SpdToolStripMenuItem" Header="{x:Static res:Resources.StringSpdCAD}" IsCheckable="True" Click="SpdToolStripMenuItem_Click"/>
                </MenuItem>
            </MenuItem>
            <MenuItem Header="{x:Static res:Resources.StringHelp}"  Padding="8,2,8,1">
                <MenuItem x:Name="instructionsToolStripMenuItem" Header="{x:Static res:Resources.StringInstructions}" Click="instructionsToolStripMenuItem_Click"/>
                <MenuItem x:Name="donateToolStripMenuItem" Header="{x:Static res:Resources.StringDonate}" Foreground="Red" FontWeight="Bold" Click="donateToolStripMenuItem_Click"/>
                <Separator/>
                <MenuItem x:Name="aboutToolStripMenuItem" Header="{x:Static res:Resources.StringAbout}" Click="aboutToolStripMenuItem_Click"/>
            </MenuItem>
        </Menu>
        <TabControl Grid.Row="1" x:Name="TabControlMain" SelectedIndex="0" SelectionChanged="TabControlMain_SelectionChanged">
            <TabItem Header="{x:Static res:Resources.StringTransmission}">
                <Grid x:Name="GridT">
                    <Grid.RowDefinitions>
                        <RowDefinition x:Name="UpRow" Height="349*"/>
                        <RowDefinition x:Name="DownRow" Height="280*" MinHeight="100"/>
                    </Grid.RowDefinitions>

                    <GridSplitter x:Name="GridSplitterT1" HorizontalAlignment="Stretch" VerticalAlignment="Top" Grid.Row="1" Height="4"/>

                    <oxy:PlotView x:Name="Chart1" Grid.Row="1" Margin="0,4,0,0">
                        <oxy:PlotView.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="{x:Static res:Resources.StringAddCurve}" Click="AddCurveToolStripMenuItem_Click"/>
                                <MenuItem Header="{x:Static res:Resources.StringClrCurve}" Click="ClrCurveToolStripMenuItem_Click"/>
                                <Separator/>
                                <MenuItem Header="{x:Static res:Resources.StringBranchDisplay}" SubmenuOpened="BranchDisToolStripMenuItem_Click">
                                    <MenuItem Header="{x:Static res:Resources.StringBranchfr}" IsCheckable="True" Click="WithCrankToolStripMenuItem_Click"/>
                                    <MenuItem Header="{x:Static res:Resources.StringBranchbk}" Visibility="Collapsed" IsCheckable="True" Click="WithCassToolStripMenuItem_Click"/>
                                    <MenuItem Header="{x:Static res:Resources.StringBranchin}" IsCheckable="True" Click="WithInterToolStripMenuItem_Click"/>
                                    <Separator/>
                                    <MenuItem Header="{x:Static res:Resources.StringNone}" IsCheckable="True" IsChecked="True" Click="NoneToolStripMenuItem_Click"/>
                                </MenuItem>
                                <MenuItem Header="{x:Static res:Resources.StringYAxis}" SubmenuOpened="YAxisToolStripMenuItem_Click">
                                    <MenuItem Header="{x:Static res:Resources.StringSpdRatio}" IsCheckable="True" IsChecked="True" Click="SRToolStripMenuItem_Click"/>
                                    <MenuItem Header="{x:Static res:Resources.StringGI}" IsCheckable="True" Click="GIToolStripMenuItem_Click"/>
                                    <MenuItem Header="{x:Static res:Resources.StringGearRatio}" IsCheckable="True" Click="GRToolStripMenuItem_Click"/>
                                    <MenuItem Header="{x:Static res:Resources.StringSpdCAD}" IsCheckable="True" Click="SpdToolStripMenuItem_Click"/>
                                </MenuItem>
                                <Separator/>
                                <MenuItem Header="{x:Static res:Resources.StringResetAxis}" Click="ResetAxisToolStripMenuItem_Click"/>
                            </ContextMenu>
                        </oxy:PlotView.ContextMenu>
                        <oxy:PlotView.Background>
                            <ImageBrush ImageSource="Resources/back.png" Stretch="UniformToFill"/>
                        </oxy:PlotView.Background>
                    </oxy:PlotView>

                    <Grid x:Name="GridUp" Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="InputColumn" Width="555*"/>
                            <ColumnDefinition x:Name="AnsColumn" Width="431*"/>
                        </Grid.ColumnDefinitions>
                        <GridSplitter x:Name="GridSplitter2" HorizontalAlignment="Left" VerticalAlignment="Stretch" Grid.Column="1" Width="4"/>

                        <Grid x:Name="GridInput" SizeChanged="splitContainer2_Panel1_SizeChanged">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="1*"/>
                            </Grid.ColumnDefinitions>
                            <GroupBox x:Name="groupBox2" Header="{x:Static res:Resources.StringCassette}" Margin="5" Grid.Column="1" Grid.RowSpan="2">
                                <GroupBox.Background>
                                    <ImageBrush ImageSource="Resources/飞轮-s4.png" Stretch="UniformToFill"/>
                                </GroupBox.Background>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <TextBlock Text="{x:Static res:Resources.StringSpeeds}" VerticalAlignment="Center" Margin="3"/>
                                    <TextBlock Grid.Row="1" Text="{x:Static res:Resources.StringModel}" VerticalAlignment="Center" Margin="3"/>
                                    <ComboBox x:Name="BkNumcomboBox" Grid.Column="1" Margin="3"/>
                                    <ComboBox x:Name="BkModelcomboBox" Grid.Column="1" Grid.Row="1" Margin="3"/>
                                    <DataGrid x:Name="dataGridViewBk" Grid.Row="2" Margin="3" Background="#B2FFFFFF" Grid.ColumnSpan="2" HeadersVisibility="None" GridLinesVisibility="Horizontal" HorizontalScrollBarVisibility="Disabled">
                                        <DataGrid.BorderBrush>
                                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                        </DataGrid.BorderBrush>
                                        <DataGrid.VerticalGridLinesBrush>
                                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                        </DataGrid.VerticalGridLinesBrush>
                                        <DataGrid.HorizontalGridLinesBrush>
                                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                        </DataGrid.HorizontalGridLinesBrush>
                                        <DataGrid.Columns>
                                            <DataGridTemplateColumn IsReadOnly="True">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Id}" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn IsReadOnly="True" Width="1*">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBox Text="{Binding Teeth}" MouseWheel="TextBox_MouseWheel_Bk" Tag="{Binding Id}" LostKeyboardFocus="TextBox_LostKeyboardFocus_Bk" BorderBrush="{x:Null}" Background="{x:Null}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Margin="0"/>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn IsReadOnly="True">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Grid Width="40">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="1*"/>
                                                                <ColumnDefinition Width="1*"/>
                                                            </Grid.ColumnDefinitions>
                                                            <RepeatButton Content="-"  Click="bkMinuButton_Click" CommandParameter="{Binding Id}" Grid.Column="0" Margin="0,0,1,0"/>
                                                            <RepeatButton Content="+"  Click="bkAddButton_Click" CommandParameter="{Binding Id}" Grid.Column="1" Margin="1,0,0,0"/>
                                                        </Grid>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                        </DataGrid.Columns>
                                        <Button/>

                                    </DataGrid>
                                </Grid>
                            </GroupBox>
                            <GroupBox x:Name="groupBox5" Header="{x:Static res:Resources.StringInternalHub}" Margin="5" Grid.Column="2" Grid.RowSpan="2">
                                <GroupBox.Background>
                                    <ImageBrush ImageSource="Resources/内变速-s1.png" Stretch="UniformToFill"/>
                                </GroupBox.Background>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <TextBlock Text="{x:Static res:Resources.StringSpeeds}" VerticalAlignment="Center" Margin="3"/>
                                    <TextBlock Grid.Row="1" Text="{x:Static res:Resources.StringModel}" VerticalAlignment="Center" Margin="3"/>
                                    <ComboBox x:Name="InNumcomboBox" Grid.Column="1" Margin="3"/>
                                    <ComboBox x:Name="InModelcomboBox" Grid.Column="1" Grid.Row="1" Margin="3"/>
                                    <DataGrid x:Name="dataGridViewIn" Grid.Row="2" Margin="3" Background="#B2FFFFFF" Grid.ColumnSpan="2" HeadersVisibility="None" GridLinesVisibility="Horizontal" HorizontalScrollBarVisibility="Disabled">
                                        <DataGrid.BorderBrush>
                                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                        </DataGrid.BorderBrush>
                                        <DataGrid.VerticalGridLinesBrush>
                                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                        </DataGrid.VerticalGridLinesBrush>
                                        <DataGrid.HorizontalGridLinesBrush>
                                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                        </DataGrid.HorizontalGridLinesBrush>
                                        <DataGrid.Columns>
                                            <DataGridTemplateColumn IsReadOnly="True">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Id}" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn IsReadOnly="True" Width="1*">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBox Text="{Binding Teeth, StringFormat=\{0:F3\}}" MouseWheel="TextBox_MouseWheel_In" Tag="{Binding Id}" LostKeyboardFocus="TextBox_LostKeyboardFocus_In" BorderBrush="{x:Null}" Background="{x:Null}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Margin="0"/>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn IsReadOnly="True">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Grid Width="40">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="1*"/>
                                                                <ColumnDefinition Width="1*"/>
                                                            </Grid.ColumnDefinitions>
                                                            <RepeatButton Content="-"  Click="inMinuButton_Click" CommandParameter="{Binding Id}" Grid.Column="0" Margin="0,0,1,0"/>
                                                            <RepeatButton Content="+"  Click="inAddButton_Click" CommandParameter="{Binding Id}" Grid.Column="1" Margin="1,0,0,0"/>
                                                        </Grid>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                        </DataGrid.Columns>
                                        <Button/>
                                    </DataGrid>
                                </Grid>
                            </GroupBox>
                            <Grid Grid.Column="0" Grid.RowSpan="2">
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <GroupBox Header="{x:Static res:Resources.StringCranksets}" Margin="5" Grid.Row="0">
                                    <GroupBox.Background>
                                        <ImageBrush ImageSource="Resources/牙盘1-s8.png" Stretch="UniformToFill">
                                            <ImageBrush.RelativeTransform>
                                                <TransformGroup>
                                                    <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                                                    <SkewTransform CenterY="0.5" CenterX="0.5"/>
                                                    <RotateTransform CenterY="0.5" CenterX="0.5"/>
                                                    <TranslateTransform/>
                                                </TransformGroup>
                                            </ImageBrush.RelativeTransform>
                                        </ImageBrush>
                                    </GroupBox.Background>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="{x:Static res:Resources.StringSpeeds}" VerticalAlignment="Center" Margin="3"/>
                                        <TextBlock Grid.Row="1" Text="{x:Static res:Resources.StringModel}" VerticalAlignment="Center" Margin="3"/>
                                        <ComboBox x:Name="FrNumcomboBox" Grid.Column="1" Margin="3"/>
                                        <ComboBox x:Name="FrModelcomboBox" Grid.Column="1" Grid.Row="1" Margin="3"/>
                                        <DataGrid x:Name="dataGridViewFr" Grid.Row="2" Margin="3" Background="#B2FFFFFF" Grid.ColumnSpan="2" HeadersVisibility="None" GridLinesVisibility="Horizontal" HorizontalScrollBarVisibility="Disabled">
                                            <DataGrid.BorderBrush>
                                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                            </DataGrid.BorderBrush>
                                            <DataGrid.VerticalGridLinesBrush>
                                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                            </DataGrid.VerticalGridLinesBrush>
                                            <DataGrid.HorizontalGridLinesBrush>
                                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveBorderColorKey}}"/>
                                            </DataGrid.HorizontalGridLinesBrush>
                                            <DataGrid.Columns>
                                                <DataGridTemplateColumn IsReadOnly="True">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <TextBlock Text="{Binding Id}" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn IsReadOnly="True" Width="1*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <TextBox Text="{Binding Teeth}" MouseWheel="TextBox_MouseWheel_Fr" Tag="{Binding Id}" LostKeyboardFocus="TextBox_LostKeyboardFocus_Fr" BorderBrush="{x:Null}" Background="{x:Null}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Margin="0"/>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn IsReadOnly="True">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Grid Width="40">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="1*"/>
                                                                    <ColumnDefinition Width="1*"/>
                                                                </Grid.ColumnDefinitions>
                                                                <RepeatButton Content="-"  Click="frMinuButton_Click" CommandParameter="{Binding Id}" Grid.Column="0" Margin="0,0,1,0"/>
                                                                <RepeatButton Content="+"  Click="frAddButton_Click" CommandParameter="{Binding Id}" Grid.Column="1" Margin="1,0,0,0"/>
                                                            </Grid>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                            </DataGrid.Columns>
                                            <Button/>

                                        </DataGrid>
                                    </Grid>
                                </GroupBox>
                                <GroupBox Header="{x:Static res:Resources.StringCadenceSpeed}" Margin="5" Grid.Row="1">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Slider x:Name="trackBar1" Grid.ColumnSpan="4" Margin="3,3,3,0" ValueChanged="trackBar1_ValueChanged" Minimum="1" Maximum="200" LargeChange="10" SmallChange="0.1" Value="80" TickFrequency="20"/>
                                        <Grid Grid.ColumnSpan="4" Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <Label x:Name="labelCAD" Content="80 rpm" Grid.Row="1" Margin="0" HorizontalAlignment="Center" FontWeight="Bold" FontSize="14" Foreground="Black" ContextMenuOpening="labelCAD_ContextMenuOpening">
                                                <Label.ContextMenu>
                                                    <ContextMenu>
                                                        <MenuItem Header="KM/H" Click="kMHToolStripMenuItem_Click"/>
                                                        <MenuItem Header="MPH" Click="mPHToolStripMenuItem_Click"/>
                                                    </ContextMenu>
                                                </Label.ContextMenu>
                                            </Label>
                                            <CheckBox x:Name="checkBox1" Content="{x:Static res:Resources.StringSpeed}" Grid.Column="1" Margin="3,0,3,0" Grid.Row="1" VerticalAlignment="Center" Click="checkBox1_Click"/>
                                        </Grid>
                                        <TextBlock Text="{x:Static res:Resources.StringBikeWeight}" Grid.Row="2" Grid.Column="0" Margin="3,3,0,3" VerticalAlignment="Center" TextAlignment="Right"/>
                                        <TextBlock Text="{x:Static res:Resources.StringSlope}" Grid.Row="2" Grid.Column="2" Margin="3,3,0,3" VerticalAlignment="Center" TextAlignment="Right"/>
                                        <TextBlock Text="{x:Static res:Resources.StringBodyWeight}" Grid.Row="3" Grid.Column="0" Margin="3,3,0,3" VerticalAlignment="Center" TextAlignment="Right"/>
                                        <TextBlock Text="{x:Static res:Resources.StringWindSpeed}" Grid.Row="3" Grid.Column="2" Margin="3,3,0,3" VerticalAlignment="Center" TextAlignment="Right"/>
                                        <TextBox x:Name="textBox_bikewt" Grid.Row="2" Grid.Column="1" Margin="3" TextChanged="textBox_bikewt_TextChanged" MouseWheel="textBox_MouseWheel_float" LostKeyboardFocus="textBox_bikewt_LostKeyboardFocus"/>
                                        <TextBox x:Name="textBox_slop" Grid.Row="2" Grid.Column="3" Margin="3" TextChanged="textBox_slop_TextChanged" MouseWheel="textBox_MouseWheel_signed_float" LostKeyboardFocus="textBox_slop_LostKeyboardFocus"/>
                                        <TextBox x:Name="textBox_bodywt" Grid.Row="3" Grid.Column="1" Margin="3" TextChanged="textBox_bodywt_TextChanged" MouseWheel="textBox_MouseWheel_float" LostKeyboardFocus="textBox_bodywt_LostKeyboardFocus"/>
                                        <TextBox x:Name="textBox_windspd" Grid.Row="3" Grid.Column="3" Margin="3" TextChanged="textBox_windspd_TextChanged" MouseWheel="textBox_MouseWheel_signed_float" LostKeyboardFocus="textBox_windspd_LostKeyboardFocus"/>
                                    </Grid>
                                </GroupBox>
                                <GroupBox Header="{x:Static res:Resources.StringTire}" Margin="5" Grid.Row="2">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
